{
  "name": "@januswel/dic",
  "version": "0.0.8",
  "description": "dictionary utils",
  "author": "Takagi Kensuke <janus.wel.3@gmail.com>",
  "license": "MIT",
  "main": "./src/index.js",
  "bin": "./bin/dic",
  "scripts": {
    "build": "rm -rf dist && babel src --out-dir dist",
    "lint": "cd ../ && eslint cli/src",
    "prepare": "yarn build",
    "start": "yarn build && ./bin/dic",
    "test": "yarn type-check && yarn lint && yarn unittest",
    "type-check": "cd ../ && yarn type-check",
    "unittest": "cd ../ && mocha --require babel-core/register $(find cli/src -name '*.test.js')"
  },
  "keywords": [
    "dictionary"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/januswel/dic.git"
  },
  "bugs": {
    "url": "https://github.com/januswel/dic/issues"
  },
  "homepage": "https://github.com/januswel/dic/cli#readme",
  "dependencies": {
    "cheerio": "1.0.0-rc.2",
    "commander": "2.15.0",
    "node-fetch": "2.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0"
  }
}
