{
  "name": "server",
  "version": "0.0.1",
  "description": "hosting dictionary definitions",
  "author": "Takagi Kensuke <janus.wel.3@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --watch --mode development",
    "lint": "cd ../ && eslint server/src",
    "start": "webpack-dev-server --watch --mode production",
    "test": "yarn type-check && yarn lint && yarn unittest",
    "type-check": "cd ../ && yarn type-check",
    "unittest": "cd ../ && mocha --require babel-core/register $(find server/src -name '*.test.js')"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-3": "6.24.1",
    "node-env-file": "0.1.8",
    "redux-logger": "3.0.6",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.11",
    "webpack-dev-server": "3.1.1"
  },
  "dependencies": {
    "firebase": "4.11.0",
    "history": "4.7.2",
    "invariant": "2.2.4",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0"
  }
}
